{
    "icon_theme": "Colored Zed Icons Theme Dark",
    "theme": "Ayu Mirage",

    "agent": {
        "default_model": {
            "provider": "copilot_chat",
            "model": "gpt-4o"
        }
    },
    "telemetry": { "diagnostics": false, "metrics": false },

    "node": {
      "path": "/home/dropbit/.nvm/versions/node/v22.6.0/bin/node"  // or wherever your system node is located
    },

    "project_panel": {
        "dock": "right"
    },

    "ui_font_family": "Monaspace Neon",
    "buffer_font_family": "Monaspace Neon",
    "ui_font_size": 16,
    "buffer_font_size": 16,

    "base_keymap": "VSCode",
    "vim_mode": false,
    "relative_line_numbers": true
}
